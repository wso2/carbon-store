{
    "server": {
        "https": "%https.host%",
        "http": "%http.host%"
    },
    "authentication": {
        "activeMethod": "sso",
        "methods": {
            "sso": {
                "attributes": {
                    "issuer": "social",
                    "identityProviderURL": "%https.carbon.local.ip%/samlsso",
                    "responseSigningEnabled": "true",
                    "storeAcs" : "%https.host%/store/acs.jag",
                    "useTenantKey": false
                }
            },
            "basic": {
                "attributes": {}
            }
        }
    },
    "userSpace": {
        "store": "/_system/governance/users/store"
    },
    "permissions": {
        "login": {
            "/permission/admin/login": ["ui.execute"]
        }
    }
}
