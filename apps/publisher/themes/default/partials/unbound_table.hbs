<a class="collapsing-h2" aria-expanded="true" href="#{{name}}" data-toggle="collapse">
    <h2>
        <i class="cu-btn-exp-col btn-collapsed">{{label}}</i>
    </h2>
</a>
<div id="{{name}}" class="responsive-form-container collapse in" aria-expanded="true" style="">

    {{#if subheading}}
    <table class='table table-bordered'>
        <thead class="decorated-table-heading">
            {{#each subheading}}
                <th>{{heading}}</th>
            {{/each}}
        </thead>
        <tbody>
        {{renderUnboundTablePreview .}}
        </tbody>
    </table>
    {{/if}}

</div>