<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12 advanced-search-form-container">
            <h1>{{t "Advanced Search"}}</h1>
            <h2 class="advanced-search-field-title">
                <a href="#collapseoverview" aria-expanded="false" class="collapsing-h2">
                    {{t "Find assets in all asset types"}}
                </a>
            </h2>
            <form id="search-form" class="form-horizontal">
            <div class="responsive-form-container" id="collapseoverview">
    			{{> primary-search-attributes .}}
    			{{> extended-search-attributes .}}
    			{{> advanced-search-buttons .}}
            </div>
    		</form>
        </div>
    </div>
</div>
<hr/>
<div>
    {{> advanced-search-results .}}
</div>
