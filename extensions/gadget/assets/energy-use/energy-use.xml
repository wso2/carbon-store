<?xml version="1.0" encoding="UTF-8" ?>
<Module>
	<ModulePrefs title="Energy Use"  category="Google" description="Energy use (kg of oil equivalent per capita)" height="350" tags="charts,energy,visualization">
		<Require feature="pubsub-2">
			<Param name="topics">
				<![CDATA[
				<Topic title="geodata" name="org.uec.geo.energy"
				description="Subscribes to geo data" type="object"
				subscribe="true"/>
				]]>
			</Param>
		</Require>
		<Require feature="dynamic-height"/>
	</ModulePrefs>
	<Content type="html">
		<![CDATA[
		
		<link rel="stylesheet" href="js/dojo/dijit/themes/claro/claro.css">

		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js" type="text/javascript"></script>
		<script>
		dojoConfig = {parseOnLoad:true}
		</script>
		<script src="/portal/js/dojo/dojo/dojo.js"></script>
		<script src="/portal/gadgets/energy-use/js/energy-use.js"></script>

		<div id="chart-energy" style="width: 100%; height: 100%;"></div>
		]]>
	</Content>
</Module>